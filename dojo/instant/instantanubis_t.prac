// Spawn positions
alias instantanubistrain1 "setpos -416.000000 -1608.000000 16.000000;setang 0.000000 150.000000 0.000000"
alias instantanubistrain2 "setpos -304.000000 -1608.000000 16.000000;setang 0.000000 135.000000 0.000000"
alias instantanubistrain3 "setpos -192.000000 -1608.000000 16.000000;setang 0.000000 90.000000 0.000000"
alias instantanubistrain4 "setpos -352.000000 -1696.000000 16.000000;setang 0.000000 120.000000 0.000000"
alias instantanubistrain5 "setpos -240.000000 -1696.000000 16.000000;setang 0.000000 90.000000 0.000000"




// Set unused variable
alias instantrepeat ""


// Cycle through real instant smokes
alias instantanubis .instantanubis1
alias .instantanubis1 "instantanubis1; alias instantanubis .instantanubis2; alias instantrepeat instantanubis1; instantanubis1output; instantanubis1video"
alias .instantanubis2 "instantanubis2; alias instantanubis .instantanubis3; alias instantrepeat instantanubis2; instantanubis2output; instantanubis2video"
alias .instantanubis3 "instantanubis3; alias instantanubis .instantanubis4; alias instantrepeat instantanubis3; instantanubis3output; instantanubis3video"
alias .instantanubis4 "instantanubis4; alias instantanubis .instantanubis5; alias instantrepeat instantanubis4; instantanubis4output; instantanubis4video"
alias .instantanubis5 "instantanubis5; alias instantanubis .instantanubis6; alias instantrepeat instantanubis5; instantanubis5output; instantanubis5video"
